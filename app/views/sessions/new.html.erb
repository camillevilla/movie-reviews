<%= form_for(:html => {id: "session"}) do |f| %>
<% @page_title = "UserAuth | Login" -%>
<div class= "Sign_Form">
  <h1>Log in</h1>
  <%= form_tag(:action => 'login_attempt') do %>
    <p>Email:</br> <%= text_field_tag(:email) %></p> 
    <p>Password:</br> <%= password_field_tag :password_hash %></p> 
    <%= submit_tag("Log In") %>
  <% end %>
</div>

