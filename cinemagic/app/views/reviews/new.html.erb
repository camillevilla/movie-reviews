<%= form_for [@movie, @movie.reviews.build], url: {action: "create"} do |f| %>
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
  <p>
    <%= f.label :star_rating %><br>
    <%= f.text_field :star_rating %>
  </p>
  <p>
    <%= f.submit "Create New Review"%><br>
  </p>
<% end %>
